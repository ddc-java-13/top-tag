<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".controller.DashboardFragment">

  <TextView
    android:id="@+id/text_bow"
    android:text="Bow"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@id/text_bow_season1"
    app:layout_constraintEnd_toEndOf="@id/text_bow_season1"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintWidth_percent="0.5"/>

  <TextView
    android:id="@+id/text_bow_season1"
    android:text="1 Sep - 7 Sep"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:layout_marginLeft="24dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@id/text_bow_season2"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/text_bow"
    app:layout_constraintWidth_percent="0.75"/>

  <TextView
    android:id="@+id/text_bow_season2"
    android:text="8 Sep - 15 Sep"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:layout_marginLeft="24dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@id/text_rifle"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/text_bow_season1"
    app:layout_constraintWidth_percent="0.75"/>

  <TextView
    android:id="@+id/text_rifle"
    android:text="Rifle"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:layout_marginLeft="24dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@id/text_rifle_season1"
    app:layout_constraintEnd_toEndOf="@id/text_bow_season1"
    app:layout_constraintTop_toBottomOf="@id/text_bow_season2"
    app:layout_constraintWidth_percent="0.5"/>

  <TextView
    android:id="@+id/text_rifle_season1"
    android:text=" 16 Sep - 23 Sep"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:layout_marginLeft="24dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toTopOf="@id/text_rifle_season2"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/text_rifle"
    app:layout_constraintWidth_percent="0.75"/>

  <TextView
    android:id="@+id/text_rifle_season2"
    android:text=" 24 Sep 1 Oct"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:padding="4dp"
    android:layout_marginLeft="24dp"
    android:textAlignment="center"
    android:textSize="20sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@id/text_rifle_season1"
    app:layout_constraintWidth_percent="0.75"/>

  <Button
    android:id="@+id/bow_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_bow"
    app:layout_constraintBottom_toBottomOf="@id/text_bow"
    app:layout_constraintStart_toEndOf="@id/text_bow"
    app:layout_constraintWidth_percent=".1"/>

  <Button
    android:id="@+id/bow_season1_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_bow_season1"
    app:layout_constraintBottom_toBottomOf="@id/text_bow_season1"
    app:layout_constraintStart_toEndOf="@id/text_bow_season1"
    app:layout_constraintWidth_percent=".1"/>

  <Button
    android:id="@+id/bow_season2_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_bow_season2"
    app:layout_constraintBottom_toBottomOf="@id/text_bow_season2"
    app:layout_constraintStart_toEndOf="@id/text_bow_season2"
    app:layout_constraintWidth_percent=".1"/>

  <Button
    android:id="@+id/rifle_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_rifle"
    app:layout_constraintBottom_toBottomOf="@id/text_rifle"
    app:layout_constraintStart_toEndOf="@id/text_rifle"
    app:layout_constraintWidth_percent=".1"/>

  <Button
    android:id="@+id/rifle_season1_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_rifle_season1"
    app:layout_constraintBottom_toBottomOf="@id/text_rifle_season1"
    app:layout_constraintStart_toEndOf="@id/text_rifle_season1"
    app:layout_constraintWidth_percent=".1"/>

  <Button
    android:id="@+id/rifle_season2_select"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toTopOf="@id/text_rifle_season2"
    app:layout_constraintBottom_toBottomOf="@id/text_rifle_season2"
    app:layout_constraintStart_toEndOf="@id/text_rifle_season2"
    app:layout_constraintWidth_percent=".1"/>

</androidx.constraintlayout.widget.ConstraintLayout>