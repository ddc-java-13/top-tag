<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_map">

  <fragment
    android:id="@+id/navigation_map"
    android:label="Map"
    android:name="edu.cnm.deepdive.toptag.controller.MapFragment"
    tools:layout="@layout/fragment_map"/>

  <fragment
    android:id="@+id/navigation_dashboard"
    android:label="@string/title_hunt"
    android:name="edu.cnm.deepdive.toptag.controller.HuntFragment"
    tools:layout="@layout/fragment_dashboard"/>

  <fragment
    android:id="@+id/navigation_notifications"
    android:label="@string/title_data"
    android:name="edu.cnm.deepdive.toptag.controller.NotificationsFragment"
    tools:layout="@layout/fragment_notifications"/>



</navigation>